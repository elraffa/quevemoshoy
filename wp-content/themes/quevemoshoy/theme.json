{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"color": {
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#4d34fa"
				},
				{
					"name": "Main",
					"slug": "main",
					"color": "#14111f"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#636381"
				},
				{
					"name": "Main Accent",
					"slug": "main-accent",
					"color": "#d0ccde"
				},
				{
					"name": "Primary Accent",
					"slug": "primary-accent",
					"color": "#e4e1fe"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#f6f6fc"
				},
				{
					"name": "Base",
					"slug": "base",
					"color": "#fff"
				}
			],
			"gradients": [
				{
					"name": "Purple",
					"slug": "purple",
					"gradient": "linear-gradient(135deg, #4D34FA, #ad34fa)"
				},
				{
					"name": "Blue",
					"slug": "blue",
					"gradient": "linear-gradient(135deg, #0057FF, #31B5FF)"
				},
				{
					"name": "Pink",
					"slug": "pink",
					"gradient": "linear-gradient(135deg, #FF007A, #FF81BD)"
				},
				{
					"name": "Black",
					"slug": "black",
					"gradient": "linear-gradient(135deg, #14111E, #4B4462)"
				},
				{
					"name": "Heat",
					"slug": "heat",
					"gradient": "linear-gradient(135deg, #F32758, #FFC581)"
				}
			]
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Nunito",
					"slug": "nunito",
					"fontFace": [
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nunito_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/nunito_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/nunito_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/nunito_normal_800.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/nunito_normal_900.ttf"
							]
						}
					]
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}