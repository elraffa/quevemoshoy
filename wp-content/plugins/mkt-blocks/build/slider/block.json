{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpe/slider",
  "version": "0.1.0",
  "title": "Slider",
  "category": "widgets",
  "icon": "slides",
  "description": "Example block scaffolded with Create Block tool.",
  "attributes": {
    "autoplay": {
      "type": "boolean",
      "default": true
    },
    "navigation": {
      "type": "boolean",
      "default": true
    },
    "pagination": {
      "type": "boolean",
      "default": true
    }
  },
  "example": {},
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "className": true,
    "color": {
      "background": true,
      "gradients": true,
      "link": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": [
        "top",
        "bottom"
      ]
    }
  },
  "textdomain": "slider",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}