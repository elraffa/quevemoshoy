{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mkt-blocks/test-block",
  "version": "0.1.0",
  "title": "Test Block",
  "category": "widgets",
  "description": "Example Test Block scaffolded with Create Block tool.",
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "text": {
      "type": "string",
      "source": "html",
      "selector": "h4"
    },
    "alignment": {
      "type": "string",
      "default": "left"
    },
    "backgroundColor": {
      "type": "string"
    },
    "textColor": {
      "type": "string",
      "default": "secondary"
    },
    "style": {
      "type": "object",
      "default": {
        "color": {
          "gradient": "pink",
          "text": "white"
        }
      }
    },
    "shadow": {
      "type": "boolean",
      "default": false
    },
    "shadowOpacity": {
      "type": "number",
      "default": 30
    }
  },
  "styles": [
    {
      "name": "squared",
      "label": "Squared",
      "isDefault": true
    },
    {
      "name": "rounded",
      "label": "Rounded",
      "isDefault": false
    }
  ],
  "example": {
    "attributes": {
      "text": "Hello World",
      "alignment": "center",
      "gradient": "pink",
      "textColor": "#fff",
      "shadow": true,
      "shadowOpacity": 50
    }
  },
  "textdomain": "mkt-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}